        "style-loader": "^3.3.2",
        "terser-webpack-plugin": "^5.1.2",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    },
    "dependencies": {
        "@types/dat.gui": "^0.7.7",
        "@types/stats.js": "^0.17.0",
